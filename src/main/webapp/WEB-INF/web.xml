<web-app xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0" id="WebApp_1566545562659">

<servlet>
	<servlet-name>userServlet</servlet-name>
	<servlet-class>com.wq.web.servlet.UserServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>userServlet</servlet-name>
	<url-pattern>/user</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>activeUserServlet</servlet-name>
	<servlet-class>com.wq.web.servlet.ActiveUserServlet</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>activeUserServlet</servlet-name>
	<url-pattern>/activeUserServlet</url-pattern>
</servlet-mapping>

</web-app>
